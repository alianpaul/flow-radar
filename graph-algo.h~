#ifndef DIJKSTRA_H
#define DIJKSTRA_H

#include <stdint.h>
#include <vector>

namespace ns3 {

class Graph
{

public:
   /*The Adjacent List Node;
   */
  struct AdjNode
  { 
    uint32_t from_port;
    uint32_t to_port;
    int      id;
    int      weight;
  };

  typedef std::vector<AdjNode>         AdjListEntry_t;
  typedef std::vector<AdjListEntry_t>  AdjList_t;
  
};

}

#endif
